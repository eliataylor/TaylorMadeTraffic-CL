{
  "_id": "boozallen",
  "startUrl": [
    "https://careers.boozallen.com/jobs/search"
  ],
  "selectors": [
    {
      "id": "serp-table",
      "multiple": false,
      "parentSelectors": [
        "_root"
      ],
      "selector": ".section__content",
      "type": "SelectorElement"
    },
    {
      "id": "pagination",
      "paginationType": "auto",
      "parentSelectors": [
        "serp-table",
        "pagination"
      ],
      "selector": "a.paginationNextLink",
      "type": "SelectorPagination"
    },
    {
      "id": "joblink",
      "linkType": "linkFromHref",
      "multiple": true,
      "parentSelectors": [
        "serp-table"
      ],
      "selector": "td.cell-title a",
      "type": "SelectorLink"
    },
    {
      "id": "location",
      "multiple": false,
      "parentSelectors": [
        "joblink"
      ],
      "regex": "",
      "selector": ".article--details--top div.article__content__view__field:nth-child(1)",
      "type": "SelectorText"
    },
    {
      "id": "remote",
      "multiple": false,
      "parentSelectors": [
        "joblink"
      ],
      "regex": "",
      "selector": ".article--details--top div.article__content__view__field:nth-child(2)",
      "type": "SelectorText"
    },
    {
      "id": "compensation",
      "multiple": true,
      "parentSelectors": [
        "joblink"
      ],
      "regex": "\\$([0-9,]+(\\.[0-9]{2})?) to \\$([0-9,]+(\\.[0-9]{2})?) \\(annualized USD\\)",
      "selector": ".article__content--rich-text .article__content__view__field__value",
      "type": "SelectorText"
    }
  ]
}
